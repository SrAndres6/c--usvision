@model DocumentoEstudiante
<h2>Eliminar Documento de Estudiante</h2>
<p>¿Estás seguro de eliminar el documento @Model.Nombre?</p>
<form asp-action="EliminarConfirmado" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit">Eliminar</button>
    <a href="/DocumentosEstudiante" class="btn btn-secondary">Cancelar</a>
</form>
