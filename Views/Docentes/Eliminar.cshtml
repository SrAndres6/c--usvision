@model Docente
<h2>Eliminar Docente</h2>
<p>¿Estás seguro de eliminar a @Model.Nombre @Model.Apellidos?</p>
<form asp-action="EliminarConfirmado" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit">Eliminar</button>
    <a href="/Docentes" class="btn btn-secondary">Cancelar</a>
</form>
